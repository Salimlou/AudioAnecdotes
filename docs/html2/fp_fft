#!/bin/sh
echo Running rawfile2stream...
../../bin/rawfile2stream ../../Content2/media/thought.raw > thought.str
echo Running fp_fft2...
../../Content2/03SignalProcessing/4Chamberlin-FP_FFT/fp_fft2 -f $1 < thought.str > thought.fft
echo Running fp_fft2 -i...
../../Content2/03SignalProcessing/4Chamberlin-FP_FFT/fp_fft2 -i -f $1 < thought.fft > thought.ift
echo Running playstream...
../../bin/playstream 1 44100 < thought.ift
