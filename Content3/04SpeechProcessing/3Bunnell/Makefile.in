CC = gcc
CFLAGS = @CFLAGS@ -I. -D_LE_MACHINE
LDFLAGS = @LDFLAGS@
LIBS = -lpablio -lcwav @LIBS@ 
LOCALCLEAN = 
EXEEXT=@EXEEXT@
OBJEXT=@OBJEXT@
TOOLROOT=@TOOLROOT@
LOCALOPT=@LOCALOPT@
LOCALLINK=@LOCALLINK@

ifeq ($(EXEEXT),.exe)
    CFLAGS := $(CFLAGS) -DMSC
endif

SOURCES = psola.c ppsfns.c voicingVerify.c spenvel.c ptracks.c fp_fft.c unpack.c

EXECS=PitchShift$(EXEEXT)

include @MAKEINCLUDESDIR@/build.leaf.benmake.inc

# Dependencies

PitchShift.o: pps.h
psola.o: pps.h
ppsfns.o: pps.h
voicingVerify.o: pps.h spenvel.h
spenvel.o: spenvel.h
ptracks.o: pps.h
